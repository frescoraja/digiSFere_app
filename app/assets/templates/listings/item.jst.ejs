<div class="list-item <%= window.LTYPES[listing.escape('category')] %> clearfix"
     data-id="<%= listing.id %>">
  <img class="listing-logo" src="/assets/<%= listing.escape('category') %>-lilogo.png" />
  <div class="listing-item-info">
    <strong class="category-title"><%= window.LTYPES[listing.get('category')] %></strong>
    <br>
    <a href="javascript:void(0)" class="show-info"><%= listing.escape('title') %></a>
  </div>
</div>
