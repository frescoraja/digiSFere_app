<div class="list-item"
     data-id="<%= listing.id %>">
  <img class="listing-logo <%= window.LTYPES[listing.escape('category')] %>"
       src="/assets/<%= listing.escape('category') %>-lilogo.png" />
  <div class="listing-item-info">
    <strong class="category-title"><%= window.LTYPES[listing.get('category')] %></strong>
    <br>
    <a href="javascript:void(0)" class="show-info"><%= title %></a>
  </div>
</div>
